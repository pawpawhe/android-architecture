[project]
	description = A collection of samples to discuss and showcase different architectural tools and patterns for Android apps.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/pawpawhe
[access "refs/for/refs/*"]
	read = group user/pawpawhe
	push = group user/pawpawhe
	label-Code-Review = -2..+2 group user/pawpawhe
	label-Verified = -1..+1 group user/pawpawhe
	rebase = group user/pawpawhe
	removeReviewer = group user/pawpawhe
	submit = group user/pawpawhe
[access "refs/heads/*"]
	read = group user/pawpawhe
	create = group user/pawpawhe
	push = +force group user/pawpawhe
	pushMerge = group user/pawpawhe
[access "refs/tags/*"]
	push = group user/pawpawhe
	push = +force group user/pawpawhe
